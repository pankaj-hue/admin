<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase / # Replace with the actual subdirectory your app is in

  # Redirect to index.html for all requests that are not files or directories
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^(.*)$ /your-subdirectory/index.html [L]
</IfModule>